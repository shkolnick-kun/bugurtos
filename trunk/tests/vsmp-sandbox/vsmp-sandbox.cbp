<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="vsmp-sandbox" />
		<Option pch_mode="2" />
		<Option compiler="avrgcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/vsmp-sandbox.elf" prefix_auto="1" extension_auto="0" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="avrgcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-mmcu=atmega328p" />
			<Add option='-include &quot;${PROJECT_DIR}config.h&quot;' />
			<Add option='-I &quot;${PROJECT_DIR}../../include&quot;' />
			<Add option="-ffunction-sections" />
			<Add option="-DF_CPU=16000000UL" />
		</Compiler>
		<Linker>
			<Add option="-mmcu=atmega328p" />
			<Add option="-Wl,-Map=$(TARGET_OUTPUT_FILE).map,--cref,--gc-sections" />
		</Linker>
		<ExtraCommands>
			<Add after="avr-size $(TARGET_OUTPUT_FILE)" />
			<Add after="avr-objcopy -O ihex -R .eeprom -R .eesafe $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).hex" />
			<Add after="avr-objcopy --no-change-warnings -j .eeprom --change-section-lma .eeprom=0 -O ihex $(TARGET_OUTPUT_FILE) $(TARGET_OUTPUT_FILE).eep.hex" />
			<Add after="avr-objdump -h -S $(TARGET_OUTPUT_FILE) &gt; $(TARGET_OUTPUT_FILE).lss" />
		</ExtraCommands>
		<Unit filename="../../arch/avr/gcc-vsmp/vsmp.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../../arch/avr/gcc-vsmp/vsmp.h" />
		<Unit filename="../../arch/avr/gcc/bugurt_kernel_asm.S" />
		<Unit filename="../../arch/avr/gcc/bugurt_proc_stack_init.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../../kernel/item.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="config.h" />
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<envvars />
			<code_completion />
			<debugger>
				<remote_debugging>
					<options conn_type="0" serial_baud="115200" ip_address="127.0.0.1" ip_port="1234" />
				</remote_debugging>
			</debugger>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
